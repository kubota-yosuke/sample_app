<%= form_with model: vehicle, local: true do |f| %>
  <div class="form-group">
    <%= f.label :vehicle_name %>
    <%= f.text_field :vehicle_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control" %>
  </div>
  
  <%= f.submit value="投稿", class: "btn btn-primary" %>
<% end %>
